{
	"root": true,
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error"
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": ["plugin:prettier/recommended"],
	"env": {
		"browser": true,
		"node": true,
		"jquery": true
	}
}